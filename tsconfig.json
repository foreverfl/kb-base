{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["@docusaurus/types", "@types/react", "@types/react-dom"],
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"],
      "@generated/*": [".docusaurus/*"],
      "@docusaurus/*": ["node_modules/@docusaurus/*"],
      "@theme/*": ["node_modules/@docusaurus/theme-classic/lib/*"],
      "@theme-original/*": ["node_modules/@docusaurus/theme-classic/lib/*"]
    }
  },
  "include": [
    "docusaurus.config.js",
    "sidebars.js",
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    ".docusaurus",
    "build"
  ]
}